{
  "$schema": "https://mintlify.com/docs.json",
  "api": {
    "playground": {
      "display": "simple"
    }
  },
  "colors": {
    "dark": "#435AAE",
    "light": "#435AAE",
    "primary": "#435AAE"
  },
  "contextual": {
    "options": [
      "copy",
      "chatgpt",
      "claude",
      "perplexity"
    ]
  },
  "favicon": "/favicon.png",
  "footer": {
    "socials": {
      "x": "https://x.com/golanceofficial",
      "instagram": "https://instagram.com/golanceofficial",
      "linkedin": "https://linkedin.com/company/golance",
      "youtube": "https://youtube.com/@golance_/videos"
    }
  },
  "logo": {
    "dark": "/logo/dark.png",
    "light": "/logo/light.svg"
  },
  "name": "Golance API Documentation",
  "navbar": {
    "links": [
      {
        "href": "https://golance.com/contact-us",
        "label": "Support"
      },
      {
        "href": "https://golance.com/login",
        "label": "Log in"
      }
    ],
    "primary": {
      "href": "https://golance.com/signup",
      "label": "Sign up",
      "type": "button"
    }
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "goLance Home",
          "href": "https://golance.com",
          "icon": "home"
        },
        {
          "anchor": "Blog",
          "href": "https://blog.golance.com",
          "icon": "newspaper"
        }
      ]
    },
    "groups": [
      {
        "group": "Getting started",
        "pages": [
          "pages/getting-started/introduction",
          "pages/getting-started/authentication",
          "pages/getting-started/handling-errors"
        ]
      },
      {
        "group": "API reference",
        "openapi": "openapi.json"
      }
    ]
  },
  "theme": "aspen",
  "appearance": {
    "default": "light"
  }
}